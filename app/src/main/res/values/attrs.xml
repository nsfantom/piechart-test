<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CombiList">
        <attr name="list_title" format="string" value = "Title"/>
        <attr name="multiSelect" format="boolean"/>
        <attr name="selectAllPosition" format="enum">
            <enum name="top" value="0"/>
            <enum name="bottom" value="1"/>
        </attr>
        <attr name="acceptBtnText" format="string" value = "proceed"/>
        <attr name="cancelBtnText" format="string" value = "cancel"/>
    </declare-styleable>
    <declare-styleable name="ForList">
        <attr name="header_text" format="string" value = "For"/>
        <attr name="mainColor" format="color"/>
        <attr name="secondColor" format="color"/>
        <attr name="footer_text" format="string" value="End"/>
        <attr name="text_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="DiscreteSlider">
        <attr name="tickMarkCount" format="integer" />
        <attr name="tickMarkRadius" format="dimension" />
        <attr name="position" format="integer" />
        <attr name="backdropStrokeColor" format="color" />
        <attr name="backdropStrokeWidth" format="dimension" />
        <attr name="thumb" format="reference" />
        <attr name="progressDrawable" format="reference" />
    </declare-styleable>
</resources>
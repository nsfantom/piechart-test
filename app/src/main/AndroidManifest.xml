<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tm.nsfantom.piechart">

    <application
        android:name=".PieChartApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.PieChartActivity" />
        <activity android:name=".activity.DemoActivity" />
        <activity android:name=".activity.CountryActivity" />
        <activity android:name=".activity.NotificationActivity"/>
        <activity android:name=".activity.DragDropActivity"/>
        <activity android:name=".activity.RingActivity" />
        <activity android:name=".activity.HexagonActivity" />
    </application>

</manifest>